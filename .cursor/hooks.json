{
  "version": 1,
  "hooks": {
    "sessionStart": [
      { "command": "node .cursor/hooks/session-init.js" }
    ],
    "beforeReadFile": [
      { "command": "node .cursor/hooks/protect-memory.js" }
    ],
    "afterFileEdit": [
      { "command": "node .cursor/hooks/knowledge-tracker.js" }
    ],
    "afterShellExecution": [
      { "command": "node .cursor/hooks/knowledge-tracker.js" }
    ]
  }
}
